{
    "name": "cerbero/lazy-json-pages",
    "type": "library",
    "description": "Laravel package to load paginated items of JSON APIs into lazy collections.",
    "keywords": [
        "laravel",
        "json",
        "lazy",
        "collection",
        "pagination",
        "parser",
        "lexer"
    ],
    "homepage": "https://github.com/cerbero90/lazy-json-pages",
    "license": "MIT",
    "authors": [{
        "name": "Andrea Marco Sartori",
        "email": "andrea.marco.sartori@gmail.com",
        "homepage": "https://github.com/cerbero90",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.3||^8.0",
        "cerbero/lazy-json": "^1.1",
        "illuminate/http": ">=8.37",
        "illuminate/support": ">=8.37"
    },
    "require-dev": {
        "orchestra/testbench": ">=6.0",
        "pestphp/pest": "^1.12",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Cerbero\\LazyJsonPages\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cerbero\\LazyJsonPages\\": "tests"
        }
    },
    "scripts": {
        "test": "pest",
        "check-style": "phpcs --standard=PSR12 src tests",
        "fix-style": "phpcbf --standard=PSR12 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Cerbero\\LazyJsonPages\\Providers\\LazyJsonPagesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
